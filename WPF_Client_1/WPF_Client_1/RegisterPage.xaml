<Window x:Class="WPF_Client_1.RegisterPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Register" Height="300" Width="400">
    <Grid>
        <TextBox x:Name="UsernameInput" Width="200" Height="25" Margin="100,50,0,0" HorizontalAlignment="Left" />
        <PasswordBox x:Name="PasswordInput" Width="200" Height="25" Margin="100,100,0,0" HorizontalAlignment="Left"/>
        <Button Content="Register" Width="100" Height="25" Margin="150,150,0,0" HorizontalAlignment="Left" Click="Register_Click"/>
        <TextBlock x:Name="RegisterMessage" Width="300" Height="25" Margin="50,200,0,0" HorizontalAlignment="Left" Visibility="Collapsed"/>
    </Grid>
</Window>
